<% breadcrumb :seats %>

<div class="row">
  <div class="col-lg-12">
    <div class="ibox float-e-margins">
      <div class="ibox-title">
        <h5>查看座位(<%=@session.full_name%>)</h5>
      </div>

      <% if 'seat'==params[:display_type]%>
        <%= render 'display_by_seat' %>
      <% else %>
        <%= render 'display_by_attendee' %>
      <% end %>
    </div>
  </div>
</div>

<% content_for :script do %>
  <%= javascript_include_tag 'seat_index_display_by_attendee' %>
<% end %>
